<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="person" class="com.bnaskov.Person" autowire-candidate="default">
		<constructor-arg name="id" value="777" type="int">
		</constructor-arg>
		<constructor-arg name="name" value="Mary" type="String"></constructor-arg>		
		<property name="taxId" value="123"></property>
	</bean>
</beans>
